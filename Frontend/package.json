{
  "name": "smarthome",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .tsx,.ts",
    "schema": "apollo client:download-schema --endpoint=https://domov.azurewebsites.net/graphql/ ./src/graphql/graphql-schema.json",
    "generate": "apollo client:codegen --localSchemaFile=./src/graphql/graphql-schema.json --target=typescript --includes=./src/graphql/**/*.ts --tagName=gql --addTypename --globalTypesFile=./src/graphql/graphql-global-types.ts types",
    "ui": "yarn upgrade-interactive"
  },
  "dependencies": {
    "@apollo/react-ssr": "^3.1.4",
    "@date-io/luxon": "1.3.13",
    "@material-ui/core": "^4.9.8",
    "@material-ui/icons": "^4.9.1",
    "@material-ui/pickers": "^3.2.10",
    "@types/lodash": "^4.14.150",
    "@types/luxon": "^1.22.0",
    "@typescript-eslint/eslint-plugin": "^2.26.1-alpha.7",
    "@typescript-eslint/parser": "^2.26.0",
    "apollo": "^2.25.0",
    "apollo-boost": "^0.4.7",
    "apollo-link-http": "^1.5.16",
    "classnames": "^2.2.6",
    "eslint-plugin-react": "^7.19.0",
    "graphql-tag": "^2.10.3",
    "isomorphic-unfetch": "^3.0.0",
    "lodash": "^4.17.15",
    "luxon": "^1.23.0",
    "material-table": "^1.57.2",
    "next": "9.3.2",
    "notistack": "^0.9.9",
    "now": "^18.0.0",
    "react": "16.13.1",
    "react-apollo": "^3.1.3",
    "react-dom": "16.13.1",
    "react-google-login": "^5.1.2",
    "universal-cookie": "^4.0.3"
  },
  "devDependencies": {
    "@types/node": "^13.9.5",
    "@types/react": "^16.9.27",
    "eslint": "^6.8.0",
    "typescript": "^3.8.3"
  }
}
